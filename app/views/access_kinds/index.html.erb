<p id="notice"><%= notice %></p>

<h1>Access Kinds</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @access_kinds.each do |access_kind| %>
      <tr>
        <td><%= access_kind.description %></td>
        <td><%= link_to 'Show', access_kind %></td>
        <td><%= link_to 'Edit', edit_access_kind_path(access_kind) %></td>
        <td><%= link_to 'Destroy', access_kind, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Access Kind', new_access_kind_path %>
